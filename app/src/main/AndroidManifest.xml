<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sharefare.aliraza.com">

    <application
        android:allowBackup="true"
        android:icon="@drawable/sharefare"
        android:label="Share Fare"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".AddJourneyActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity"/>
        </activity>

        <activity android:name=".AddedJourney">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity"/>
        </activity>
        <activity android:name=".SearchJourney">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity"/>
        </activity>
        <activity android:name=".BookJourney">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity"/>
        </activity>
        <activity android:name=".ForgetPassword" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".EmailActivity" />
        <activity android:name=".PhoneNumberActivity"></activity>
    </application>

</manifest>